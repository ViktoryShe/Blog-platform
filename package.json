    "@testing-library/user-event": "^13.5.0",
    "antd": "^5.19.2",
    "axios": "^1.7.2",
    "classnames": "^2.5.1",
    "date-fns": "^3.6.0",
    "immer": "^10.1.1",
    "lodash": "^4.17.21",
    "lodash.debounce": "^4.0.8",
    "marked-react": "^2.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-detect-offline": "^2.4.5",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.52.1",
    "react-markdown": "^9.0.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.25.0",
    "react-scripts": "5.0.1",
    "redux-thunk": "^3.1.0",
